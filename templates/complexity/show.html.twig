{% extends 'base.html.twig' %}

{% block title %}Complexity{% endblock %}

{% block body %}
    <h1>Complexity</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ complexity.id }}</td>
            </tr>
            <tr>
                <th>NameLevel</th>
                <td>{{ complexity.nameLevel }}</td>
            </tr>
            <tr>
                <th>Level</th>
